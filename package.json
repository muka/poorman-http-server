{
  "name": "poorman-http-server",
  "version": "1.0.2",
  "description": "HTTP server for static file ",
  "type": "module",
  "engines": {
    "node": ">= 22.11 < 23"
  },
  "main": "bin/http-server.js",
  "bin": {
    "http-server": "bin/http-server"
  },
  "dependencies": {
    "commander": "13.1.0",
    "dotenv": "16.4.7",
    "express": "^5.1.0",
    "winston": "^3.17.0"
  },
  "scripts": {
    "test": "ts-mocha -n loader=ts-node/esm -p tsconfig.json --check-leaks src/**/*.test.ts",
    "start": "./bin/http-server.ts",
    "dev": "nodemon -e ts,js,json --watch './src/**/*' --watch './bin/**/*' --exec './bin/http-server.ts --port 3000'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muka/http-server.git"
  },
  "author": "Luca Capra <luca.capra@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muka/http-server/issues"
  },
  "homepage": "https://github.com/muka/http-server#readme",
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@stylistic/eslint-plugin-ts": "4.2.0",
    "@types/debug": "4.1.12",
    "@types/expect": "1.20.4",
    "@types/express": "^5.0.1",
    "@types/koa": "2.15.0",
    "@types/koa-router": "7.4.8",
    "@types/mocha": "10.0.10",
    "@types/node": "~22",
    "@types/optimist": "0.0.33",
    "@types/pump": "1.1.3",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/eslint-plugin": "~1.1",
    "eslint": "9.24.0",
    "eslint-plugin-mocha": "10.5.0",
    "globals": "15.14.0",
    "nodemon": "3.1.9",
    "prettier": "3.4.2",
    "rimraf": "~6.0",
    "supertest": "7.1.0",
    "ts-api-utils": "~2.0",
    "ts-mocha": "11.1.0",
    "tsx": "4.19.3",
    "typescript": "5.7.3",
    "typescript-eslint": "8.19.1"
  }
